#PF-Exer-35
def count_names(name_list):
    count1 = 0
    count2 = 0
    for word in name_list:
        if word.endswith("at") and len(word) == 3:
            count1 += 1 
        if "at" in word:
            count2 += 1
    print("_at -> ",count1)
    print("%at% -> ",count2)

name_list=["Hat","Cat","rabbit","matter"]
count_names(name_list)

