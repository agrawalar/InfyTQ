#PF-Assgn-34
def find_pairs_of_numbers(num_list,n):
    count=0
    x = len(num_list)
    for i in range(x):
        for j in range(i+1,x):
            if num_list[i]+num_list[j]==n:
                count+=1
    return count
    

num_list=[1, 2, 4, 5, 6]
n=6
print(find_pairs_of_numbers(num_list,n))

